if (( $#commands[jq] )) && (( $#commands[less] ));then
  local file jq_cmd
  jq_cmd="."
  case $# in
    1)
      file=$1
      ;;
    2)
      jq_cmd="$1"
      file="$2"
      ;;
    *)
      print "Usage: $0 [COMMAND] FILE" 1>&2
      return 2
      ;;
  esac

  jq -C "$jq_cmd" "$file" | less -R

else
  print "jq or less not installed" 1>&2
  return 1
fi
